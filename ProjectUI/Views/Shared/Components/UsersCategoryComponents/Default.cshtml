@model List<Category>
<!-- ======= Resume Section ======= -->
<section id="category" class="resume section-bg">
    <div class="container">
        <div class="section-title">
            <h2>Category</h2>
        </div>
        @foreach (var item in Model)
        {
            <div class="row">
                <div class="col-lg-12" data-aos="fade-up">
                    <br>
                    <br>
                    <div class="resume-item pb-0">
                        <h4>@item.title</h4>
                        <p><em>
                            @item.text
                        </em></p>
                        <img src="@item.img" style="height:300px; width:300px; align-items:center;">
                    </div>
                </div>
            </div>
        }

    </div>
</section><!-- End Resume Section -->